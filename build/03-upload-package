#!/bin/bash

set -e

cd work/result

dput bleemeo-agent bleemeo-agent_*.changes

REPREPRO_ROOT="/srv/www/packages.bleemeo.com/htdocs/bleemeo-agent/"
docker run --rm -v ${REPREPRO_ROOT}:${REPREPRO_ROOT} -v ${HOME}/.gnupg:/root/.gnupg bleemeo/reprepro reprepro -Vb ${REPREPRO_ROOT} processincoming default

